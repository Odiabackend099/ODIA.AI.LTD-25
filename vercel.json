{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/odiadev-admin-dashboard/dist/$1"
    },
    {
      "source": "/widget/(.*)",
      "destination": "/odiadev-chat-widget/dist/$1"
    },
    {
      "source": "/widget",
      "destination": "/odiadev-chat-widget/dist/index.html"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    },
    {
      "source": "/api",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-vercel-remove-auth-header",
          "value": "1"
        },
        {
          "key": "x-vercel-protection-bypass",
          "value": "true"
        }
      ]
    }
  ]
}